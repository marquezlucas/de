    "\n",
    "import requests\n",
    "import pandas as pd\n",
    "import json\n",
    "\n",
    "url = \"https://cat-fact.herokuapp.com/facts\"\n",
    "headers = {\"Accept-Encoding\": \"gzip, deflate\"}\n",
    "\n",
    "response = requests.get(url, headers=headers)\n",
    "data = json.loads(response.text)\n",
    "data\n",
    "df = pd.DataFrame(data)\n",
    "df.head()"
